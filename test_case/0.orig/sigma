FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      sigma;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [-1 -3 3 0 0 2 0];

internalField uniform 61000; // ~1/1.64e-5, carbon

boundaryField
{
	".*"
	{
		type	calculated;
		value	$internalField;
	}
}

